{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": [
      "es2015",
      "es2016"
    ],
    "types": [
      "node"
    ],
    "declaration": true,
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
  },
  "exclude": [
    "lib",
    "node_modules",
    "src/test/**.ts"
  ],
  "include": [
    "src/**.ts",
  ],
}